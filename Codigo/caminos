'''import os, folium as fl, webbrowser as wb, networkx as nx, pandas as pd
df = pd.read_csv("calles_de_medellin_con_acoso.csv", sep=';')
df.head()
def interfacita():
    print("                        ||~~**MENU DE OPCIONES**~~||                  ")
    print("1. Buscar camino mas corto.")
    print("2. Buscar camino con menos acoso.")
    opcion1 = int(input())
    if opcion1 == 1:
        opcion1 = "length"
        color = "black"
    else: 
        opcion1 = "harassmentRisk"
        color = "purple"
    print("Digite las coordenadas de origen: ")
    cord1 = input()
    print("Digite las coordenadas de llegada: ")
    cord2 = input()
    mapita = nx.from_pandas_edgelist(df, "origin", "destination", opcion1)
    djk_path = nx.dijkstra_path(mapita,cord1, cord2, opcion1)
    for i in range(len(djk_path)):
        djk_path[i] = eval(djk_path[i])[::-1]
    html = fl.Map(djk_path[0],20)
    rayita = fl.PolyLine(djk_path,color,10).add_to(html)
    html.save(os.path.join("SEEHEATMAP.html"))
    wb.open_new_tab("SEEHEATMAP.html")

interfacita()'''